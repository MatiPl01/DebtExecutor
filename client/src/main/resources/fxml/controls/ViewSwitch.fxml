<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<fx:root styleClass="view-switch" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../../css/controls/ViewSwitch.css"/>
    </stylesheets>
    <Button fx:id="listViewButton" mnemonicParsing="false"
            styleClass="view-switch__button" text="List"/>
    <Button fx:id="graphViewButton" mnemonicParsing="false"
            styleClass="view-switch__button" text="Graph"/>
</fx:root>
